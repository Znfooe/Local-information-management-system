  };
  useEffect(() => {
    fetchApis();
  }, [search]);
  const handleSave = async () => {
    await db.saveApi(currentApi);
    setIsOpen(false);
    setCurrentApi({});
    fetchApis();
    toast.success(t("common.saved"));
