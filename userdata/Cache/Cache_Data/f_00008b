        method: testMethod,
        url: testUrl,
        headers,
        data: testMethod !== "GET" ? data : void 0
      });
      setTestStatus(res.status);
      setTestResponse(JSON.stringify(res.data, null, 2));
      toast.success("Request finished");
    } catch (error) {
